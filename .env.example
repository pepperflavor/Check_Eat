# Database Configuration
DATABASE_URL="postgresql://postgres:your_db_password@localhost:5432/check_eat_db"
DB_NAME=check_eat_db
DB_PASSWORD=your_db_password

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
